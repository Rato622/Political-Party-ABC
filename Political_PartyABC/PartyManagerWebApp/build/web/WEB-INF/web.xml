<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AddMemberServlet</servlet-name>
        <servlet-class>za.gov.party.web.AddMemberServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchMemberServlet</servlet-name>
        <servlet-class>za.gov.party.web.SearchMemberServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActionMemberServlet</servlet-name>
        <servlet-class>za.gov.party.web.ActionMemberServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditMemberServlet</servlet-name>
        <servlet-class>za.gov.party.web.EditMemberServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateMemberServlet</servlet-name>
        <servlet-class>za.gov.party.web.UpdateMemberServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AllMembersServlet</servlet-name>
        <servlet-class>za.gov.party.web.AllMembersServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddMemberServlet</servlet-name>
        <url-pattern>/AddMemberServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchMemberServlet</servlet-name>
        <url-pattern>/SearchMemberServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActionMemberServlet</servlet-name>
        <url-pattern>/ActionMemberServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditMemberServlet</servlet-name>
        <url-pattern>/EditMemberServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateMemberServlet</servlet-name>
        <url-pattern>/UpdateMemberServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AllMembersServlet</servlet-name>
        <url-pattern>/AllMembersServlet.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
